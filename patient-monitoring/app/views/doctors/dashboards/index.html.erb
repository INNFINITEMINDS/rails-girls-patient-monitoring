<div class="row">
	<div class="col-lg-4"></div>
<div class="col-lg-4">
	<h1>Search a patient</h1>

<%= form_tag(patients_path, :method => "get", id: "search-form") do %>
  <%= text_field_tag :search, params[:search], placeholder: "Search Posts" %>
  <%= submit_tag "Search" %>
<% end %>
</div>
</div>



<div class="container">
<h1>Patients</h1>
<ul>
  <% @patients.each do |c| %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><%=  link_to c.email, c %></h3>
      </div>
    </div>
  <% end %>
</ul>
</div>